<div class="container">
<% flash.each do |name,msg| %>
    <div class="alert alert-danger" role="alert- alert-dismissible fade show">
    <%= msg %>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
  <span aria-hidden="true">&times;</span>
  </button>
  </div>
    <%end%>
    </div>